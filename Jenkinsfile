pipeline {
  agent any
  stages {
    stage('Compilación') {
      steps {
        echo '✅ Compilando...'
      }
    }
    stage('Pruebas Unitarias') {
      steps {
        echo '✅ Ejecutando pruebas...'
      }
    }
    stage('Despliegue en Staging') {
      steps {
        echo '✅ Desplegando en staging...'
      }
    }
    stage('Producción') {
      steps {
        echo '✅ Desplegando en producción...'
      }
    }
  }
}
